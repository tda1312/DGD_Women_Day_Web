    <p><input type="submit" name="ctl00$ctl00$mainContent$leftContent$btnEnquiry" value="SEND ENQUIRY" id="mainContent_leftContent_btnEnquiry" class="btn btn-success btn-lg" />
        <span id="PostBackMsg" style="display:none;">Sending enquiry. Please wait.</span></p>

    

    <script type="text/javascript">
        $(document).ready(function () {
            //called when key is pressed in textbox
            $("#mainContent_leftContent_txtBudget").keypress(function (e) {
                //if the letter is not digit then display error and don't type anything
                if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                    //display error message
                    //$("#errmsg").html("Digits Only").show().fadeOut("slow");
                    return false;
                }
            });
            
            $('form').submit(function () {
                $('#PostBackMsg').show();
                $('#mainContent_leftContent_btnEnquiry').hide();
            });

            
        });

    </script>


            </div>
            <div class="col-sm-4">

                
                
