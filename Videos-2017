    <script type="text/javascript">
        $(document).ready(function () {
            $('#videoModal').on('show.bs.modal', function (event) {
                var tag = $(event.relatedTarget)
                var sid = tag.data('vp')
                var videourl 
                if (sid == 1) { videourl = '//www.youtube.com/embed/' + tag.data('vidid') }
                else { videourl = '//player.vimeo.com/video/' + tag.data('vidid')}
                var vidtitle = tag.data('title')
                var modal = $(this)
                modal.find('.modal-title').text(vidtitle)
                $("#idVideo").attr('src', videourl);

                $("#videoModal").on('hide.bs.modal', function () {
                    $("#idVideo").attr('src', '');
                });
            });
            $('.carousel').carousel({
                interval: 8000,
                wrap: true
            });
        });

    </script>

    
